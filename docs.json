{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "Plex Development",
  "colors": {
    "primary": "#7B68EE",
    "light": "#7B68EE",
    "dark": "#7B68EE"
  },
  "logo": {
    "light": "/logo/logo.svg",
    "dark": "/logo/logo.svg",
    "href": "https://plexdevelopment.net"
  },
  "favicon": "/favicon.svg",
  "thumbnails": {
    "appearance": "dark"
  },
  "styling": {
    "codeblocks": "dark",
    "eyebrows": "section"
  },
  "icons": {
    "library": "lucide"
  },
  "fonts": {
    "heading": {
      "family": "Geist Mono"
    }
  },
  "appearance": {
    "default": "dark",
    "strict": true
  },
  "background": {
    "decoration": "windows"
  },
  "navbar": {
    "links": [
      {
        "label": "Dashboard",
        "href": "https://dashboard.plexdevelopment.net"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Website",
      "href": "https://plexdevelopment.net"
    }
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Welcome!",
            "pages": [
              "index"
            ]
          },
                    {
            "group": "Getting Started",
            "pages": [
              "getting-started/requirements",
              "getting-started/installation"
            ]
          }
        ]
      }
    ],
    "global": {
      "dropdowns": [
        {
          "dropdown": "Community Documentation",
          "href": "https://plex.amogusreal.tech",
          "icon": "users"
        },
        {
          "dropdown": "Official Documentation",
          "href": "https://docs.plexdevelopment.net",
          "icon": "book"
        }
      ],
      "anchors": [
        {
          "anchor": "Paste Tool",
          "href": "https://paste.plexdevelopment.net",
          "icon": "clipboard"
        },
        {
          "anchor": "Status Page",
          "href": "https://status.plexdevelopment.net",
          "icon": "activity"
        }
      ]
    }
  },
  "footer": {
    "socials": {
      "discord": "https://discord.gg/plexdev"
    },
    "links": [
      {
        "header": "Legal",
        "items": [
          {
            "href": "https://plexdevelopment.net/privacy-policy",
            "label": "Privacy Policy"
          },
          {
            "href": "https://plexdevelopment.net/tos",
            "label": "Terms of Service"
          }
        ]
      }
    ]
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude"
    ]
  },
  "integrations": {
    "telemetry": {
      "enabled": false
    }
  }
}